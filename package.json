{
  "name": "@elonehoo/is-window",
  "version": "0.0.5",
  "description": "Check if the given value is a window object.",
  "license": "MIT",
  "author": "Elone Hoo <huchengyea@163.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elonehoo/is-window"
  },
  "homepage": "https://github.com/elonehoo/is-window#readme",
  "bugs": "https://github.com/elonehoo/is-window/issues",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./*": "./*"
  },
  "files": [
    "dist",
    "*.d.ts"
  ],
  "scripts": {
    "build": "rimraf dist && unbuild",
    "release": "bumpp package.json --commit --push --tag && pnpm -r publish --access public",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@elonehoo/eslint-config": "2.0.0",
    "@elonehoo/rimraf": "^0.0.1",
    "@types/node": "22.13.13",
    "@vitest/coverage-v8": "^2.1.9",
    "bumpp": "9.11.1",
    "c8": "^10.1.3",
    "eslint": "9.23.0",
    "pnpm": "10.10.0",
    "typescript": "5.8.2",
    "unbuild": "^3.5.0",
    "vite": "6.2.3",
    "vitest": "2.1.9"
  }
}
